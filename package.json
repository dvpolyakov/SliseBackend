  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prisma:migrate:up && npm run prisma:generate && nest start",
    "start:watch": "npm run prisma:migrate:up && npm run prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prisma:migrate:up": "npm run prisma -- migrate deploy",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:watch": "npm run prisma -- generate -- --watch",
    "prisma:silent-watch": "npm run prisma -- generate -- --watch &",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:prod": "node dist/main.js"
  },
  "dependencies": {
    "@maticnetwork/eth-decoder": "^0.0.4",
